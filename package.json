{
  "name": "template-vue",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@7.17.0",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "clean": "rimraf dist pnpm-lock.yaml node_modules",
    "dev": "vite",
    "build": " vite build",
    "test": "vite build --mode development",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "check:types": "vue-tsc --noEmit",
    "lint": "eslint --fix src/**/*.{js,ts,tsx,vue}",
    "lint:style": "stylelint --fix **/*.{vue,html,css,scss,sass}",
    "commit": "cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.52.0",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "colord": "^2.9.3",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@lxchapu/eslint-config-vue": "^0.1.0",
    "@lxchapu/stylelint-config-vue": "^1.0.1",
    "@types/lodash-es": "^4.17.7",
    "@unocss/eslint-config": "^0.52.0",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/runtime-core": "^3.3.4",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "cz-emoji-conventional": "^1.0.1",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "terser": "^5.17.4",
    "typescript": "^5.0.4",
    "unocss": "^0.52.0",
    "unplugin-auto-import": "^0.16.1",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.8",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-markdown": "^0.23.5",
    "vue-tsc": "^1.6.5"
  },
  "browserslist": [
    "last 2 versions",
    "> 0.3%",
    "not dead",
    "Firefox ESR",
    "Chrome >=70"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji-conventional"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,vue}": "eslint --fix",
    "**/*.{vue,html,css,scss,sass}": "stylelint --fix",
    "**/*.{js,ts,tsx,md,html,vue,css,scss}": "prettier --write"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": []
    }
  }
}
